{
  "name": "smart-contract-api",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "dev": "nodemon -e ts -w ./src -x npm run dev:serve",
    "dev:serve": "node --inspect -r dotenv/config -r ts-node/register -- src/index.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint --fix \"src/**/*.ts\"",
    "start": "npm run build && node -r dotenv/config dist/index.js",
    "test": "npm run lint && ts-mocha --exit \"src/**/*.spec.ts\"",
    "test:watch": "ts-mocha -w --exit \"src/**/*.spec.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/input-output-hk/smart-contract-api.git"
  },
  "keywords": [
    "Apollo",
    "ApolloServer",
    "GraphQL",
    "Cardano"
  ],
  "contributors": [
    "Rhys Bartels-Waller <rhys.bartelswaller@iohk.io> (https://iohk.io)",
    "Sam Jeston <sam.jeston@iohk.io> (https://iohk.io)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/input-output-hk/smart-contract-api/issues"
  },
  "homepage": "https://github.com/input-output-hk/smart-contract-api#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/expect": "^1.20.4",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.11.1",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "apollo-server": "^2.4.8",
    "axios": "^0.18.0",
    "decompress": "^4.2.0",
    "dotenv": "^6.2.0",
    "graphql": "^14.1.1",
    "nock": "^10.0.6",
    "rimraf": "^2.6.3",
    "tcp-port-used": "^1.0.1"
  }
